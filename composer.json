{
	"name": "mickael-blue/cakephp-imagine-plugin",
	"type": "cakephp-plugin",
	"description": "Imagine Plugin for CakePHP",
	"keywords": ["cakephp","media","image","image-processing"],
	"homepage": "https://github.com/burzum/cakephp-imagine-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Florian Kr√§mer",
			"homepage": "http://florian-kraemer.net"
		}
	],
	"require": {
		"cakephp/cakephp": "~5.0",
		"imagine/imagine": "1.3.*",
		"cakephp/plugin-installer": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*"
	},
	"autoload": {
 		"psr-4": {
 			"Burzum\\Imagine\\": "src",
 			"Burzum\\Imagine\\Test\\Fixture\\": "tests\\Fixture"
 		}
 	},
 	"autoload-dev": {
 		"psr-4": {
 			"Cake\\Test\\": "/vendor/cakephp/cakephp/tests",
 			"Burzum\\Imagine\\Test\\": "tests"
 		}
 	},
	"config": {
		"allow-plugins": {
			"cakephp/plugin-installer": true
		}
	}
}
